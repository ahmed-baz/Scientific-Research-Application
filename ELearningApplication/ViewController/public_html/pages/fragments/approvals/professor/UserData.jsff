                              selectionListener="#{bindings.Awards2.collectionModel.makeCurrent}" rowSelection="single"
                              fetchSize="#{bindings.Awards2.rangeSize}"
                              filterModel="#{bindings.Awards2Query.queryDescriptor}"
                              queryListener="#{bindings.Awards2Query.processQuery}" filterVisible="true" varStatus="vs"
                              id="t3" columnStretching="last">

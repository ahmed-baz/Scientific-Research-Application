                        <af:column sortProperty="#{bindings.Awards2.hints.Notes.name}" filterable="true" sortable="true"
                                   headerText="#{bindings.Awards2.hints.Notes.label}" id="c6">
                            <af:inputText value="#{row.bindings.Notes.inputValue}"
                                          label="#{bindings.Awards2.hints.Notes.label}"
                                          required="#{bindings.Awards2.hints.Notes.mandatory}"
                                          columns="#{bindings.Awards2.hints.Notes.displayWidth}"
                                          maximumLength="#{bindings.Awards2.hints.Notes.precision}"
                                          shortDesc="#{bindings.Awards2.hints.Notes.tooltip}" id="it12">
                                <f:validator binding="#{row.bindings.Notes.validator}"/>
                                <af:convertNumber groupingUsed="false"
                                                  pattern="#{bindings.Awards2.hints.Notes.format}"/>

                    <f:facet name="toolbar"/>
                    <af:table value="#{bindings.Qualifiactions2.collectionModel}" var="row"
                              rows="#{bindings.Qualifiactions2.rangeSize}"
                              emptyText="#{bindings.Qualifiactions2.viewable ? 'No data to display.' : 'Access Denied.'}"
                              rowBandingInterval="0"
                              selectedRowKeys="#{bindings.Qualifiactions2.collectionModel.selectedRow}"
                              selectionListener="#{bindings.Qualifiactions2.collectionModel.makeCurrent}"
                              rowSelection="single" fetchSize="#{bindings.Qualifiactions2.rangeSize}"
                              filterModel="#{bindings.Qualifiactions2Query.queryDescriptor}"
                              queryListener="#{bindings.Qualifiactions2Query.processQuery}" filterVisible="true"
                              varStatus="vs" id="t2" columnStretching="last">
                        <af:column sortProperty="#{bindings.Qualifiactions2.hints.QualTypeCode.name}" filterable="true"
                                   sortable="true" headerText="#{bindings.Qualifiactions2.hints.QualTypeCode.label}"

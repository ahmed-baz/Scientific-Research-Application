                                                required="#{bindings.Qualifiactions2.hints.QualTypeCode.mandatory}"
                                                shortDesc="#{bindings.Qualifiactions2.hints.QualTypeCode.tooltip}"
                                                id="soc1">
                                <f:selectItems value="#{row.bindings.QualTypeCode.items}" id="si1"/>
                                <f:validator binding="#{row.bindings.QualTypeCode.validator}"/>
                            </af:selectOneChoice>
                        </af:column>
                        <af:column sortProperty="#{bindings.Qualifiactions2.hints.ScienceName.name}" filterable="true"
                                   sortable="true" headerText="#{bindings.Qualifiactions2.hints.ScienceName.label}"
                                   id="c10">
                            <af:inputListOfValues id="scienceNameId"
                                                  popupTitle="Search and Select: #{bindings.Qualifiactions2.hints.ScienceName.label}"
                                                  value="#{row.bindings.ScienceName.inputValue}"
                                                  model="#{row.bindings.ScienceName.listOfValuesModel}"
                                                  required="#{bindings.Qualifiactions2.hints.ScienceName.mandatory}"
                                                  columns="#{bindings.Qualifiactions2.hints.ScienceName.displayWidth}"
                                                  shortDesc="#{bindings.Qualifiactions2.hints.ScienceName.tooltip}">
                                <f:validator binding="#{row.bindings.ScienceName.validator}"/>
                            </af:inputListOfValues>
                        </af:column>
                        <af:column sortProperty="#{bindings.Qualifiactions2.hints.QualDate.name}" filterable="true"
                                   sortable="true" headerText="#{bindings.Qualifiactions2.hints.QualDate.label}"
                                   id="c4">
                            <af:inputDate value="#{row.bindings.QualDate.inputValue}"
                                          label="#{bindings.Qualifiactions2.hints.QualDate.label}"
                                          required="#{bindings.Qualifiactions2.hints.QualDate.mandatory}"
                                          columns="#{bindings.Qualifiactions2.hints.QualDate.displayWidth}"
                                          shortDesc="#{bindings.Qualifiactions2.hints.QualDate.tooltip}" id="id3">
                                <f:validator binding="#{row.bindings.QualDate.validator}"/>
                                <af:convertDateTime pattern="#{bindings.Qualifiactions2.hints.QualDate.format}"/>
                            </af:inputDate>
                            <f:facet name="filter">
                                <af:inputDate value="#{bindings.Qualifiactions2Query.queryDescriptor.filterConjunctionCriterion.criterionMap.QualDate.value}"
                                              id="id1">
                                    <af:convertDateTime pattern="#{bindings.Qualifiactions2.hints.QualDate.format}"/>
                                </af:inputDate>
                            </f:facet>
                        </af:column>
                        <af:column sortProperty="#{bindings.Qualifiactions2.hints.UniversityName.name}"
                                   filterable="true" sortable="true"
                                   headerText="#{bindings.Qualifiactions2.hints.UniversityName.label}" id="c12">
                            <af:inputListOfValues id="universityNameId"
                                                  popupTitle="Search and Select: #{bindings.Qualifiactions2.hints.UniversityName.label}"
                                                  value="#{row.bindings.UniversityName.inputValue}"
                                                  model="#{row.bindings.UniversityName.listOfValuesModel}"
                                                  required="#{bindings.Qualifiactions2.hints.UniversityName.mandatory}"
                                                  columns="#{bindings.Qualifiactions2.hints.UniversityName.displayWidth}"
                                                  shortDesc="#{bindings.Qualifiactions2.hints.UniversityName.tooltip}">
                                <f:validator binding="#{row.bindings.UniversityName.validator}"/>
                            </af:inputListOfValues>
                        </af:column>
                        <af:column sortProperty="#{bindings.Qualifiactions2.hints.Notes.name}" filterable="true"
                                   sortable="true" headerText="#{bindings.Qualifiactions2.hints.Notes.label}" id="c9">
                            <af:inputText value="#{row.bindings.Notes.inputValue}"
                                          label="#{bindings.Qualifiactions2.hints.Notes.label}"
                                          required="#{bindings.Qualifiactions2.hints.Notes.mandatory}"
                                          columns="#{bindings.Qualifiactions2.hints.Notes.displayWidth}"
                                          maximumLength="#{bindings.Qualifiactions2.hints.Notes.precision}"
                                          shortDesc="#{bindings.Qualifiactions2.hints.Notes.tooltip}" id="it10">
                                <f:validator binding="#{row.bindings.Notes.validator}"/>
                            </af:inputText>
                        </af:column>
                    </af:table>
                    <f:facet name="statusbar"/>
                </af:panelCollection>
            </af:showDetailItem>
            <af:showDetailItem id="Awards" text="Awards" icon="/resources/images/--04-128.png">
                <af:panelCollection id="pc2" styleClass="AFStretchWidth">
                    <f:facet name="menus"/>
                    <f:facet name="toolbar"/>
                    <af:table value="#{bindings.Awards2.collectionModel}" var="row" rows="#{bindings.Awards2.rangeSize}"

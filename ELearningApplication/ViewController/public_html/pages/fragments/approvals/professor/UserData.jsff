    <af:popup childCreation="deferred" autoCancel="disabled" id="p1" contentDelivery="lazyUncached"
              binding="#{viewScope.ApprovalsBean.profAccept}">
        <af:dialog id="d1" type="none" contentWidth="500" contentHeight="150">
            <f:facet name="buttonBar">
                <af:button text="Ok" id="b3" action="SelectApproval"
                           binding="#{viewScope.ApprovalsBean.submitButtonProApproval}"/>
            </f:facet>
            <af:panelGroupLayout id="pgl1" halign="center" valign="middle" layout="horizontal">
                <af:outputText value="Professor Accepted Successfully" id="ot1"
                               inlineStyle="color:Green;font-size:25px;margin-right:5px;"/>
                <af:image source="/resources/images/JD-16-128.png" id="i1"/>
            </af:panelGroupLayout>
        </af:dialog>
    </af:popup>
    <af:popup childCreation="deferred" autoCancel="disabled" id="p2" binding="#{viewScope.ApprovalsBean.profFailed}"
              contentDelivery="lazyUncached">
        <af:dialog id="d2" contentWidth="500" contentHeight="150" type="cancel" binding="#{AcceptBean.proFaild}">
            <af:panelGroupLayout id="pgl2" halign="center" valign="middle" layout="horizontal">
                <af:outputText value="User Not Found" id="ot2"
                               inlineStyle="color:Maroon;font-size:25px;margin-right:5px;"/>
                <af:image source="/resources/images/stop_wrong_cross-128.png" id="i2"/>
            </af:panelGroupLayout>
            <f:facet name="buttonBar"/>
        </af:dialog>
    </af:popup>
</ui:composition>
